  "links": [],
  "panels": [
    {
      "title": "Request Rate (Traffic)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(http_server_duration_milliseconds_count[1m]))",
          "legendFormat": "Total Requests"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "title": "Error Rate (Errors)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(http_server_duration_milliseconds_count{http_status_code=~\"5..\"}[1m]))",
          "legendFormat": "5xx Errors"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "title": "P99 Latency (Latency)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_server_duration_milliseconds_bucket[1m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "datasource": "Prometheus"
    },
     {
      "title": "P90 Latency (Latency)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.90, sum(rate(http_server_duration_milliseconds_bucket[1m])) by (le))",
          "legendFormat": "P90"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "title": "Top Endpoints by Duration (P90)",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "topk(10, histogram_quantile(0.90, sum(rate(http_server_duration_milliseconds_bucket[5m])) by (le, http_route)))",
          "legendFormat": "{{http_route}}"
        }
      ],
      "datasource": "Prometheus"
    }
  ],
